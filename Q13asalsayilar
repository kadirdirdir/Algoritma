package kadirdirdir;

public class birvebinasal {

	public static void main(String[] args) {
		 int asal; 
         for(int i=2; i<=1000; i++){ 
             asal=0; 
             for(int j=2; j<=i/2; j++){ 
                 if(i%j==0) asal++; 
             } 
             if(asal==0) System.out.println(+i); 
         } 

	}

}
