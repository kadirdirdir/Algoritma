package kadirdirdir;

import java.util.Scanner;

public class carpimtablosu {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
			Scanner dude=new Scanner(System.in);
			 
			for(int i=1;i<=10;i++){
			for(int j=1;j<=10;j++){
			System.out.println(i+" x "+j+" = "+i*j);
			}

	}
	}
}



